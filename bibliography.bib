	keywords = {acoustics, design of experiment, model order reduction, parameterized partial differential equations, robotic data acquisition, variational data assimilation},
	pages = {933--965},
	file = {Full Text PDF:/home/loiseau/Zotero/storage/LNAQYKML/Maday et al. - 2015 - A parameterized-background data-weak approach to v.pdf:application/pdf;Snapshot:/home/loiseau/Zotero/storage/BB89EZPX/nme.html:text/html},
