	year = {2022},
	note = {arXiv:2207.04941 [astro-ph, physics:cond-mat, physics:physics]},
	keywords = {Computer Science - Machine Learning, Physics - Data Analysis, Statistics and Probability, Astrophysics - Instrumentation and Methods for Astrophysics, Condensed Matter - Disordered Systems and Neural Networks, Condensed Matter - Statistical Mechanics},
	file = {arXiv Fulltext PDF:/home/loiseau/Zotero/storage/27YUPNDX/Marchand et al. - 2022 - Wavelet Conditional Renormalization Group.pdf:application/pdf;arXiv.org Snapshot:/home/loiseau/Zotero/storage/7H8TDL9A/2207.html:text/html},
