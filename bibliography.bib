	note = {arXiv:2106.01211 [physics]
type: article},
	keywords = {Mathematics - Optimization and Control, Physics - Fluid Dynamics, Mathematics - Dynamical Systems, 14M15, 15A03, 34A45, 34C20, 49J27, 49J30, 49M37, 76D55, 90C06, 90C26, 90C45, 93A15, 93C10},
	file = {arXiv Fulltext PDF:/home/loiseau/Zotero/storage/JZCSQ3CP/Otto et al. - 2022 - Optimizing Oblique Projections for Nonlinear Syste.pdf:application/pdf;arXiv.org Snapshot:/home/loiseau/Zotero/storage/QWGHJ7LB/2106.html:text/html},
