@article{johnson_nonlinear_nodate,
	title = {Nonlinear data-driven control via {Koopman} models: the interplay of stabilization and subspace closure error},
	abstract = {We explore the stabilization of an unknown nonlinear dynamic system. We use a state-inclusive Koopman model of the system dynamics. This model includes an approximation of the evolution of a nonlinear augmented state as a linear combination of the state and augmented state. When the evolution of the nonlinear augmented state is not captured exactly by the Koopman model, control action designed against the Koopman model can fail. We present two control solutions to address this problem. The first is to account for the error in the Koopman model directly. We design a sliding mode controller to do this. The second is to change the Koopman model and an adaptive controller’s parameters in tandem to ensure asymptotic stability of the origin of the closed-loop system. This may mean that the Koopman model itself is less accurate, however, the controlled system is guaranteed to be stable. This result implies that certain scenarios merit datadriven control strategies distinct from system identification strategies that prioritize best fit.},
	language = {en},
	author = {Johnson, Charles A and Yeung, Enoch},
	file = {Johnson and Yeung - Nonlinear data-driven control via Koopman models .pdf:/home/loiseau/Zotero/storage/RBJGZ7A2/Johnson and Yeung - Nonlinear data-driven control via Koopman models .pdf:application/pdf},
}

@misc{strasser_robust_2023,
	title = {Robust data-driven control for nonlinear systems using the {Koopman} operator},
	abstract = {Data-driven analysis and control of dynamical systems have gained a lot of interest in recent years. While the class of linear systems is well studied, theoretical results for nonlinear systems are still rare. In this paper, we present a data-driven controller design method for discrete-time control-affine nonlinear systems. Our approach relies on the Koopman operator, which is a linear but infinite-dimensional operator lifting the nonlinear system to a higher-dimensional space. Particularly, we derive a linear fractional representation of a lifted bilinear system representation based on measured data. Further, we restrict the lifting to finite dimensions, but account for the truncation error using a finite-gain argument. We derive a linear matrix inequality based design procedure to guarantee robust local stability for the resulting bilinear system for all error terms satisfying the finite-gain bound and, thus, also for the underlying nonlinear system. Finally, we apply the developed design method to the nonlinear Van der Pol oscillator.},
	publisher = {arXiv},
	author = {Strässer, Robin and Berberich, Julian and Allgöwer, Frank},
	month = apr,
	year = {2023},
	note = {arXiv:2304.03519 [cs, eess, math]},
