	file = {arXiv Fulltext PDF:/home/loiseau/Zotero/storage/CMTLCGR5/Krishna et al. - 2023 - Finite Time Lyapunov Exponent Analysis of Model Pr.pdf:application/pdf;arXiv.org Snapshot:/home/loiseau/Zotero/storage/H7IKSDN9/2304.html:text/html},
}

@misc{haughn_deep_2023,
