@misc{feldman_how_2021,
	title = {How {Do} {You} {Want} {Your} {Greedy}: {Simultaneous} or {Repeated}?},
	shorttitle = {How {Do} {You} {Want} {Your} {Greedy}},
	doi = {10.48550/arXiv.2009.13998},
	abstract = {We present SimultaneousGreedys, a deterministic algorithm for constrained submodular maximization. At a high level, the algorithm maintains \${\textbackslash}ell\$ solutions and greedily updates them in a simultaneous fashion. SimultaneousGreedys achieves the tightest known approximation guarantees for both \$k\$-extendible systems and the more general \$k\$-systems, which are \$(k+1){\textasciicircum}2/k = k + {\textbackslash}mathcal\{O\}(1)\$ and \$(1 + {\textbackslash}sqrt\{k+2\}){\textasciicircum}2 = k + {\textbackslash}mathcal\{O\}({\textbackslash}sqrt\{k\})\$, respectively. This is in contrast to previous algorithms, which are designed to provide tight approximation guarantees in one setting, but not both. We also improve the analysis of RepeatedGreedy, showing that it achieves an approximation ratio of \$k + {\textbackslash}mathcal\{O\}({\textbackslash}sqrt\{k\})\$ for \$k\$-systems when allowed to run for \${\textbackslash}mathcal\{O\}({\textbackslash}sqrt\{k\})\$ iterations, an improvement in both the runtime and approximation over previous analyses. We demonstrate that both algorithms may be modified to run in nearly linear time with an arbitrarily small loss in the approximation. Both SimultaneousGreedys and RepeatedGreedy are flexible enough to incorporate the intersection of \$m\$ additional knapsack constraints, while retaining similar approximation guarantees: both algorithms yield an approximation guarantee of roughly \$k + 2m + {\textbackslash}mathcal\{O\}({\textbackslash}sqrt\{k+m\})\$ for \$k\$-systems and SimultaneousGreedys enjoys an improved approximation guarantee of \$k+2m + {\textbackslash}mathcal\{O\}({\textbackslash}sqrt\{m\})\$ for \$k\$-extendible systems. To complement our algorithmic contributions, we provide a hardness result which states that no algorithm making polynomially many oracle queries can achieve an approximation better than \$k + 1/2 + {\textbackslash}varepsilon\$. We also present SubmodularGreedy.jl, a Julia package which implements these algorithms and may be downloaded at https://github.com/crharshaw/SubmodularGreedy.jl . Finally, we test the effectiveness of these algorithms on real datasets.},
	publisher = {arXiv},
	author = {Feldman, Moran and Harshaw, Christopher and Karbasi, Amin},
	month = jul,
	year = {2021},
	note = {arXiv:2009.13998 [cs, math]},
	keywords = {Mathematics - Optimization and Control, Computer Science - Machine Learning, Computer Science - Data Structures and Algorithms},
	file = {arXiv Fulltext PDF:/home/loiseau/Zotero/storage/MT3D9FC2/Feldman et al. - 2021 - How Do You Want Your Greedy Simultaneous or Repea.pdf:application/pdf;arXiv.org Snapshot:/home/loiseau/Zotero/storage/2KAQB4YM/2009.html:text/html},
}

@misc{harshaw_power_2021,
	title = {The {Power} of {Subsampling} in {Submodular} {Maximization}},
	doi = {10.48550/arXiv.2104.02772},
	abstract = {We propose subsampling as a unified algorithmic technique for submodular maximization in centralized and online settings. The idea is simple: independently sample elements from the ground set, and use simple combinatorial techniques (such as greedy or local search) on these sampled elements. We show that this approach leads to optimal/state-of-the-art results despite being much simpler than existing methods. In the usual offline setting, we present SampleGreedy, which obtains a \$(p + 2 + o(1))\$-approximation for maximizing a submodular function subject to a \$p\$-extendible system using \$O(n + nk/p)\$ evaluation and feasibility queries, where \$k\$ is the size of the largest feasible set. The approximation ratio improves to \$p+1\$ and \$p\$ for monotone submodular and linear objectives, respectively. In the streaming setting, we present SampleStreaming, which obtains a \$(4p +2 - o(1))\$-approximation for maximizing a submodular function subject to a \$p\$-matchoid using \$O(k)\$ memory and \$O(km/p)\$ evaluation and feasibility queries per element, where \$m\$ is the number of matroids defining the \$p\$-matchoid. The approximation ratio improves to \$4p\$ for monotone submodular objectives. We empirically demonstrate the effectiveness of our algorithms on video summarization, location summarization, and movie recommendation tasks.},
	publisher = {arXiv},
	author = {Harshaw, Christopher and Kazemi, Ehsan and Feldman, Moran and Karbasi, Amin},
	month = apr,
	year = {2021},
	note = {arXiv:2104.02772 [cs, math]},
	keywords = {Mathematics - Optimization and Control, Computer Science - Machine Learning, Computer Science - Data Structures and Algorithms},
	file = {arXiv Fulltext PDF:/home/loiseau/Zotero/storage/KBBSFKNH/Harshaw et al. - 2021 - The Power of Subsampling in Submodular Maximizatio.pdf:application/pdf;arXiv.org Snapshot:/home/loiseau/Zotero/storage/EARKBWA7/2104.html:text/html},
}

